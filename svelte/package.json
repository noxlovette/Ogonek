{
  "name": "ogonek",
  "version": "1.12.4",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node build/index.js",
    "test": "vitest run && pnpx cypress run",
    "test:unit": "vitest",
    "test:unit:run": "vitest run",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "generate-types": "openapi-typescript ./openapi.json -o src/lib/types/api/gen/openapi.d.ts && node scripts/generate-routes.js && node scripts/generate-mock.js",
    "generate-mock": "node scripts/generate-mock.js",
    "cy:open": "pnpx cypress open"
  },
  "type": "module",
  "dependencies": {
    "@sentry/sveltekit": "^10.15.0",
    "@sveltejs/adapter-node": "^5.3.2",
    "@types/node": "^24.5.2",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "fflate": "^0.8.2",
    "ioredis": "^5.8.0",
    "jose": "^6.1.0",
    "lucide-svelte": "^0.544.0",
    "papaparse": "^5.5.3",
    "pino": "^9.12.0",
    "rehype-format": "^5.0.1",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "tippy.js": "^6.3.7",
    "unified": "^11.0.5",
    "unsplash-js": "^7.0.20",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.36.0",
    "@faker-js/faker": "^10.0.0",
    "@inlang/paraglide-js": "^2.4.0",
    "@sveltejs/kit": "^2.43.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/papaparse": "^5.3.16",
    "cypress": "^15.3.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "knip": "^5.64.1",
    "nanoid": "^5.1.6",
    "openapi-typescript": "^7.9.1",
    "orval": "^7.13.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.39.6",
    "svelte-check": "^4.3.2",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.17.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
