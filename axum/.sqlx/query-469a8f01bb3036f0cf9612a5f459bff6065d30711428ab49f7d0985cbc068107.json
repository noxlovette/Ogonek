  "query": "\n        SELECT id,\n            title,\n            priority,\n            completed,\n            due_date\n        FROM tasks\n        WHERE (assignee = $1 OR created_by = $1)\n        AND completed = false\n        ORDER BY created_at DESC\n        LIMIT 3\n        ",
