{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            l.id,\n            l.title,\n            l.topic,\n            l.markdown,\n            l.assignee,\n            l.created_by,\n            l.created_at,\n            l.updated_at,\n            u.name as assignee_name\n        FROM lessons l\n        LEFT JOIN \"user\" u ON l.assignee = u.id\n        WHERE (l.assignee = $1 OR l.created_by = $1)\n        ORDER BY l.created_at DESC\n        ",
