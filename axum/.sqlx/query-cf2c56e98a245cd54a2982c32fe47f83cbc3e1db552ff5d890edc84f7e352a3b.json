  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            t.id,\n            t.title,\n            t.markdown,\n            t.priority,\n            t.completed,\n            t.due_date,\n            t.file_path,\n            t.assignee,\n            t.created_by,\n            t.created_at,\n            t.updated_at,\n            u.name as assignee_name\n        FROM tasks t\n        LEFT JOIN \"user\" u ON t.assignee = u.id\n        WHERE (t.assignee = $1 OR t.created_by = $1)\n        ",
